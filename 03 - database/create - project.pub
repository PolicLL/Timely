DROP SCHEMA IF EXISTS `timely`;

CREATE SCHEMA `timely`;
USE `timely` ;



CREATE TABLE IF NOT EXISTS `timely`.`project` (
  `id` BIGINT(20) NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(255) NULL DEFAULT NULL,
  `duration` varchar(255),
  `start_time` timestamp,
  `end_time` timestamp ,
  PRIMARY KEY (`id`))
ENGINE=InnoDB
AUTO_INCREMENT = 1;


